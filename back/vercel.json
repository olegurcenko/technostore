{
	"version": 2,
	"headers": [
		{
		  "source": "/api/(.*)",
		  "headers": [
			{
			  "key": "Access-Control-Allow-Origin",
			  "value": "*"
			},
			{
			  "key": "Access-Control-Allow-Methods",
			  "value": "GET,HEAD,PUT,PATCH,POST,DELETE"
			},
			{
			  "key": "Access-Control-Allow-Credentials",
			  "value": "true"
			}
		  ]
		}
	  ],
	"builds": [
	 {
	  "src": "index.js",
	  "use": "@vercel/node",
	  "config": { "includeFiles": ["dist/**"] }
	 }
	],
	"routes": [
	 {
	  "src": "/(.*)",
	  "dest": "index.js"
	 }
	]
   }